#!/usr/local/opt/python/bin/python3.7
# EASY-INSTALL-ENTRY-SCRIPT: 'ament-pep8==0.5.2','console_scripts','ament_pep8'
__requires__ = 'ament-pep8==0.5.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ament-pep8==0.5.2', 'console_scripts', 'ament_pep8')()
    )
